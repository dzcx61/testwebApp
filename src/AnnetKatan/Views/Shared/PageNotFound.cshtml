@model AnnetKatan.Models.Image

@{
  ViewBag.Title = "Page Not Found";
}

<div class="row">
  <div class="col-sm-7 col-md-7">
    <hgroup>
      <h2>We are sorry, the page you requested cannot be found.</h2>
      <h3>The URL may be misspelled or the page you are looking for is no longer available.</h3>
    </hgroup>
  </div>
  <div class="col-sm-5 col-md-5">
    <img class="img-responsive img-thumbnail img-noradius" src=@Model.Url>
  </div>
</div>